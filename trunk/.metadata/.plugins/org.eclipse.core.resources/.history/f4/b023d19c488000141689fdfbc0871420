package ejercicio2;

import java.util.Calendar;

public class TestDepartamento {

	public static void main(String[] args) {
		Departamento d1=new Departamento(20, "informática");
		
		Empleado e1=new Empleado("12345678A", "JUAN MARTÍN PÉREZ", "PROGRAMADOR", 2007, 1000, d1);
		Empleado e2=new Empleado("t7654321Z", "ANA LÓPEZ GONZÁLEZ", "ANALISTA", 2004, 1500, d1);
		Empleado e3=new Empleado("123321", "ANA", "ANALISTA", 2004, 1500, d1);
		Empleado e4=new Empleado("423456789", "ANA", "ANALISTA", 2004, 1500, d1);
		
		System.out.println();
		System.out.println(e1);
		System.out.println();
		System.out.println(e2);
	}
}
