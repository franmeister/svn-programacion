package r1;

public class Cola {
	private int pos=0;
	private int[] cola=new int[100];
	
	public Cola crear(){
		return new Cola();
	}
	
	public void encolar(int num){
		cola[pos]=num;
		pos++;
	}
	
	public int desencolar(){
		int num=cola[0];
		for(int i=0;i<pos;i++){
			cola[i]=cola[i+1];
		}
		return num;
	}
	
	public int frente(){
		return cola[0];
	}
}
